{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "_id",
    "id",
    "featureId",
    "nome",
    "descricao",
    "__v"
  ],
  "properties": {
    "_id": {
      "$id": "#/properties/_id",
      "type": "object",
      "title": "The _id Schema",
      "required": [
        "$oid"
      ],
      "properties": {
        "$oid": {
          "$id": "#/properties/_id/properties/$oid",
          "type": "string",
          "title": "The $oid Schema",
          "default": "",
          "examples": [
            "5d9e816c746803053044e283"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "1"
      ],
      "pattern": "^(.*)$"
    },
    "featureId": {
      "$id": "#/properties/featureId",
      "type": "string",
      "title": "The Featureid Schema",
      "default": "",
      "examples": [
        "F0001"
      ],
      "pattern": "^(.*)$"
    },
    "nome": {
      "$id": "#/properties/nome",
      "type": "string",
      "title": "The Nome Schema",
      "default": "",
      "examples": [
        "Feature teste 1"
      ],
      "pattern": "^(.*)$"
    },
    "descricao": {
      "$id": "#/properties/descricao",
      "type": "string",
      "title": "The Descricao Schema",
      "default": "",
      "examples": [
        "Descricao Feature 01"
      ],
      "pattern": "^(.*)$"
    },
    "__v": {
      "$id": "#/properties/__v",
      "type": "object",
      "title": "The __v Schema",
      "required": [
        "$numberInt"
      ],
      "properties": {
        "$numberInt": {
          "$id": "#/properties/__v/properties/$numberInt",
          "type": "string",
          "title": "The $numberint Schema",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}